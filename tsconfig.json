{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@widgets/*": [
				"src/widgets/*"
			],
			"@pages/*": [
				"src/pages/*"
			],
			"@assets/*": [
				"src/assets/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@components/*": [
				"src/components/*"
			]
		}
	}
}
